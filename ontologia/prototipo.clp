;;; Classes----------------------------------------------------------------

(defclass Autor
    (is-a USER)
    (role concrete)
    (pattern-match reactive)
    (slot escribe_subgenero
        (type INSTANCE)
        (create-accessor read-write))
    (slot nombre
        (type STRING)
        (create-accessor read-write))
    (slot cluster
        (type STRING)
        (create-accessor read-write))
)

(defclass Lector
    (is-a USER)
    (role concrete)
    (pattern-match reactive)
    (multislot genero
        (type INSTANCE)
        (create-accessor read-write))
    (multislot subgenero           
        (type INSTANCE)
        (create-accessor read-write))
    (multislot autor
        (type INSTANCE)
        (create-accessor read-write))
    (slot tiempo_total
        (type INTEGER)
        (create-accessor read-write))
    (slot tiempo_diario
        (type INTEGER)
        (create-accessor read-write))
    (slot lugar
        (type STRING)
        (create-accessor read-write))
    (slot momento
        (type STRING)
        (create-accessor read-write))
    (slot se_fija_valoraciones
        (type SYMBOL)
        (create-accessor read-write))
    (slot modas
        (type SYMBOL)
        (create-accessor read-write))
    (slot edad
        (type INTEGER)                          
        (create-accessor read-write))
    (slot tiempo_lectura
        (type INTEGER)
        (create-accessor read-write))
    (multislot decada
        (type INTEGER)                          
        (create-accessor read-write))
)

