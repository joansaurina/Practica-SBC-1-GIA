    (test (>= ?popularidad 25))
    ?s <- (object (is-a Sugerencia) (nombre ?nombres) (calificacion ?p) (argumento $?a))
    (test (eq ?nombrel ?nombres))
    =>
    (bind ?p (+ ?p 8))
    (bind $?a (insert$ $?a (+ (length$ $?a) 1) "El libro es popular: +8 puntos!."))
    (send ?s put-calificacion ?p)
    (send ?s put-argumento $?a)
    (assert (valorando-popularidad ?nombrel))
)

(defrule computar-puntuaciones::valoracion "Regla para calcular la calificacion de los libros segun su valoracion"
    ?u <- (object (is-a Usuario) (se_fija_valoraciones ?se_fija_valoraciones))
