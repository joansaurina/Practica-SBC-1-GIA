    (printout t "Por favor, seleccione el momento en el que suele leer: " crlf)
    (printout t "Opciones: 1.Manana 2.Tarde 3.Noche 4.Cualquiera" crlf)
    
    (bind ?opciones (create$ 1 2 3 4))
    (bind ?momento 0)
    
    (while (not (member$ ?momento ?opciones))
