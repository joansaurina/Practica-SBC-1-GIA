    )
    
    (switch ?momento
        (case 1 then (bind ?momento "Manana"))
        (case 2 then (bind ?momento "Tarde"))
        (case 3 then (bind ?momento "Noche"))
        (case 4 then (bind ?momento "Cualquiera"))
    )
    
    (modify-instance ?u (momento ?momento))
    (printout t "" crlf)
)

