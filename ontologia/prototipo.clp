;;; Declaracion de reglas ------------------------------------------------------------------

; Preferencias-recopilacion -------------------------------------------------

;Usuario preguntas personales
(defrule preferencias-recopilacion::establecer_nombre "Regla para establecer el nombre del usuario"
    =>
    (printout t "Por favor, introduzca su nombre: ")
    (bind ?nombre (read))
    (make-instance ?nombre of Usuario (nombre ?nombre))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::establecer_edad "Regla para establecer la edad del usuario ya creado"
    ?u <- (object (is-a Usuario) (nombre ?nombre))
    =>
    (printout t "Por favor, introduzca su edad: ")
    (bind ?edad (read))
    (while (not (numberp ?edad))
        (printout t "Por favor, introduzca su edad: ")
        (bind ?edad (read)))
    (modify-instance ?u (edad ?edad))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::establecer_tiempo_lectura "Regla para establecer el tiempo de lectura del usuario diario"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad))
    =>
    (printout t "Por favor, introduzca el tiempo que dedica a la lectura (en minutos) cada dia: ")
    (bind ?tiempo_diario (read))
    (while (not (numberp ?tiempo_diario))
        (printout t "Por favor, introduzca el tiempo que dedica a la lectura (en minutos) cada dia: ")
        (bind ?tiempo_diario (read)))
    (modify-instance ?u (tiempo_diario ?tiempo_diario))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::tiempo_lectura_total "Regla para medir tiempo que quiere dedicar a leer el usuario"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario))
    =>
    (printout t "Por favor, diga el tiempo que dedicaria como maximo para leer un libro (en semanas):  ")
    (bind ?tiempo_total (read))
    (while (not(numberp ?tiempo_total))
        (printout t "Por favor, diga el tiempo que dedicaria como maximo para leer un libro (en semanas):  ")
        (bind ?tiempo_total (read)))
    
    (modify-instance ?u (tiempo_total ?tiempo_total))
    (printout t "" crlf)
)
(defrule preferencias-recopilacion::lugar_lectura "Regla para establecer el tiempo de lectura del usuario diario (opciones)"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total))
    =>
    (printout t "Por favor, seleccione el lugar donde suele leer: " crlf)
    (printout t " Opciones: 1.Transporte publico  2.Casa 3.Parque 4.Cafeteria 5.Cualquiera" crlf)
    
    (bind ?opciones (create$ 1 2 3 4 5))
    (bind ?lugar_lectura 0)
    
    (while (not (member$ ?lugar_lectura ?opciones))
        (printout t "Ingrese el numero correspondiente a su eleccion: " crlf)
        (bind ?lugar_lectura (read))
    )
    
    (switch ?lugar_lectura
        (case 1 then (bind ?lugar_lectura "Transporte publico"))
        (case 2 then (bind ?lugar_lectura "Casa"))
        (case 3 then (bind ?lugar_lectura "Parque"))
        (case 4 then (bind ?lugar_lectura "Cafeteria"))
        (case 5 then (bind ?lugar_lectura "Cualquiera"))
    )
    
    (modify-instance ?u (lugar ?lugar_lectura))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::momento "Regla para ver el momento de lectura"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total) (lugar ?lugar_lectura))
    =>
    (printout t "Por favor, seleccione el momento en el que suele leer: " crlf)
    (printout t "Opciones: 1.Manana 2.Tarde 3.Noche 4.Cualquiera" crlf)
    
    (bind ?opciones (create$ 1 2 3 4))
    (bind ?momento 0)
    
    (while (not (member$ ?momento ?opciones))
        (printout t "Ingrese el numero correspondiente a su eleccion: " crlf)
        (bind ?momento (read))
    )
    
    (switch ?momento
        (case 1 then (bind ?momento "Manana"))
        (case 2 then (bind ?momento "Tarde"))
        (case 3 then (bind ?momento "Noche"))
        (case 4 then (bind ?momento "Cualquiera"))
    )
    
    (modify-instance ?u (momento ?momento))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::modas "Regla para ver si se fija en las modas"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total) (lugar ?lugar_lectura) (momento ?momento))
    =>
    (printout t "Usted se fija en las modas: " crlf)
    (printout t "Opciones: 1.Si 2.No" crlf)
    
    (bind ?opciones (create$ 1 2))
    (bind ?modas 0)
    
    (while (not (member$ ?modas ?opciones))
        (printout t "Ingrese el numero correspondiente a su eleccion: " crlf)
        (bind ?modas (read))
    )
    
    (switch ?modas
        (case 1 then (bind ?modas TRUE))
        (case 2 then (bind ?modas FALSE))
    )
    
    (modify-instance ?u (modas ?modas))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::valoracion "Regla para ver si se fija en las valoraciones"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total) (lugar ?lugar_lectura) (momento ?momento) (modas ?modas))
    =>
    (printout t "Usted se fija en las valoraciones de los libros que lee: " crlf)
    (printout t "Opciones: 1.Si 2.No" crlf)
    
    (bind ?opciones (create$ 1 2))
    (bind ?valoracion 0)
    
    (while (not (member$ ?valoracion ?opciones))
        (printout t "Ingrese el numero correspondiente a su eleccion: " crlf)
        (bind ?valoracion (read))
    )
    
    (switch ?valoracion
        (case 1 then (bind ?valoracion TRUE))
        (case 2 then (bind ?valoracion FALSE))
    )
    
    (modify-instance ?u (se_fija_valoraciones ?valoracion))
    (printout t "" crlf)
)

(defrule preferencias-recopilacion::genero_fav "Regla para escoger sus generos favoritos"
   ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total) (lugar ?lugar_lectura) (momento ?momento) (modas ?modas) (se_fija_valoraciones ?valoracion))
   =>
   (printout t "Que generos te gusta leer?" crlf)
   (printout t "(Selecciona uno o mas, ingresa el numero correspondiente a cada genero)" crlf)
   (printout t "1. Realistic" crlf)
   (printout t "2. Romantic" crlf)
   (printout t "3. Science Fiction" crlf)
   (printout t "4. Fantasy" crlf)
   (printout t "5. Adventure" crlf)
   (printout t "6. Historical" crlf)
   (printout t "0. Terminar la seleccion" crlf)
   
   (bind ?genero_fav (create$))
   (while TRUE
       (printout t "Ingresa el numero del genero que te gusta (o 0 para terminar): ")
       (bind ?opcion (read))
       (bind ?genero_nombre "")
       (switch ?opcion
            (case 1 then (bind ?genero_nombre "Realistic"))
            (case 2 then (bind ?genero_nombre "Romantic"))
            (case 3 then (bind ?genero_nombre "Science Fiction"))
            (case 4 then (bind ?genero_nombre "Fantasy"))
            (case 5 then (bind ?genero_nombre "Adventure"))
            (case 6 then (bind ?genero_nombre "Historical"))
        
        )
            
       (if (and (numberp ?opcion) (> ?opcion 0) (<= ?opcion 6))
           then (if (not (member$ ?genero_nombre ?genero_fav))
               then (bind ?genero_fav (create$ ?genero_fav ?genero_nombre))
               else (printout t "Ya seleccionaste ese genero. Por favor, selecciona uno diferente." crlf)
           )
           else (if (= ?opcion 0)
                   then (break)
                   else (printout t "Opción invalida. Por favor, selecciona un numero valido." crlf)
           )
       )
   )    
   
    (bind ?generos (find-all-instances ((?g Genero )) (member$ ?g:nombre ?genero_fav)))
    (modify-instance ?u (genero ?generos))

)

(defrule preferencias-recopilacion::subgeneros-favoritos
   ?u <- (object (is-a Usuario) (nombre ?nombre) (genero $?genero_fav))
   =>
    (bind ?subgeneros (find-all-instances ((?s Subgenero )) (member$ ?s:subgenero_de ?genero_fav)))

    (printout t "Que subgeneros te gusta leer?" crlf)
    (printout t "(Selecciona uno o mas, ingresa el numero correspondiente a cada subgenero)" crlf)

    (bind ?i 1)
    (foreach ?subgenre ?subgeneros

        (bind ?nombre_sub (send ?subgenre get-nombre))
        (printout t ?i ". " ?nombre_sub crlf)
        (bind ?i (+ ?i 1))
 
    )
    (printout t "0. Terminar la seleccion" crlf)
    
    (bind ?subgenero_fav (create$))
       (while TRUE
           (printout t "Ingresa el numero del subgenero que te gusta (o 0 para terminar): ")
           (bind ?opcion (read))
           
           (if (and (numberp ?opcion) (> ?opcion 0) (< ?opcion ?i))
               then
               (if (not (member$ (nth$ ?opcion ?subgeneros) ?subgenero_fav))
                   then (bind ?subgenero_fav (create$ ?subgenero_fav (nth$ ?opcion ?subgeneros)))
                   else (printout t "Ya seleccionaste ese subgenero. Por favor, selecciona uno diferente." crlf)
               )
               else (if (= ?opcion 0)
                       then (break)
                       else (printout t "Opcion invalida. Por favor, selecciona un numero valido." crlf)
               )
           )
       )
       
    (modify-instance ?u (subgenero ?subgenero_fav))
)
       
(defrule preferencias-recopilacion::tiempo_lectura "Regla para medir el tiempo de lectura del usuario"
    ?u <- (object (is-a Usuario) (nombre ?nombre) (edad ?edad) (tiempo_diario ?tiempo_diario) (tiempo_total ?tiempo_total) (lugar ?lugar_lectura) (momento ?momento) (modas ?modas) (se_fija_valoraciones ?valoracion) (genero $?genero_fav) (subgenero $?subgenero_fav))
    =>
    (printout t "Por favor, lea el siguiente parrafo cronometrando cuando ha tardado: " crlf)
    (printout t "" crlf)
    (printout t "Titulo: Las Estrellas de la Eternidad" crlf)
    (printout t "Capitulo 5: El Bosque Susurrante" crlf)
    (printout t "La suave brisa recorria los caminos empedrados de Lumara, el antiguo pueblo oculto detras de las montanas esmeralda. Las hojas de los arboles brillaban con los colores de un crepusculo eterno, reflejando tonos de naranja, morado y rosa que solo se podrian encontrar en los suenos mas vividos de un artista. Ariana caminaba descalza, dejando que las frescas piedras de cuarzo masajearan sus pies cansados mientras se abria paso a traves de la niebla de terciopelo." crlf)
    (printout t "A lo lejos, el Bosque Susurrante llamaba su nombre, las antiguas secuoyas se inclinaban suavemente, como si quisieran contarle los secretos mas oscuros del universo. Cada paso que daba la acercaba mas a la verdad que tanto ansiaba, pero tambien la envolvia mas profundamente en el misterio que el bosque guardaba." crlf)
    (printout t "Una luz eterea comenzo a filtrarse entre las ramas, creando patrones enigmaticos que danzaban en el suelo forestal. Las sombras jugaban entre los arboles, susurrando las historias de los antiguos, de los que vinieron antes y de los que vendran despues. Una sensacion de asombro y temor lleno el corazon de Ariana mientras avanzaba, sintiendo la energia vibrante del lugar llenar su ser." crlf)
    (printout t "De repente, una figura aparecio entre las sombras. Era una criatura de una belleza indescriptible, con ojos que contenian las estrellas del cosmos y alas que brillaban con los colores de la aurora. La criatura extendio su mano, invitando a Ariana a acercarse." crlf)
    (printout t "No temas, buscadora de verdades -dijo con una voz que resono como una melodia celestial. - Estas aqui para descubrir, para aprender y para crecer. Pero recuerda, no todo lo que brilla lleva la luz de la verdad." crlf)
    (printout t "Ariana, con el corazon latiendo con fuerza, acepto la mano de la criatura, permitiendose ser guiada a traves de los caminos ocultos del Bosque Susurrante. Los arboles comenzaron a hablar, sus hojas susurraban palabras de sabiduria, y las flores brillaban con una luz suave, iluminando las paginas oscuras de los secretos que estaban por revelarse." crlf)
    (printout t "Asi, en los brazos de lo desconocido, Ariana comenzo su viaje a traves de las estrellas de la eternidad, buscando las respuestas que la ayudarian a desentranar los misterios del universo y de su propio corazon." crlf)

    (printout t "" crlf)
    (printout t "El tiempo de lectura del parrafo ha sido de (segundos): " crlf)
    (bind ?tiempo_lectura (read))
    (while (not (numberp ?tiempo_lectura))
        (printout t "El tiempo de lectura del parrafo ha sido de: " crlf)
        (bind ?tiempo_lectura (read)))
    (modify-instance ?u (tiempo_lectura ?tiempo_lectura))

    (focus datos-procesamiento)
)


