    (slot tiempo_diario
        (type INTEGER)
        (create-accessor read-write))
    (slot lugar
        (type STRING)
        (create-accessor read-write))
    (slot momento
        (type STRING)
        (create-accessor read-write))
    (slot se_fija_valoraciones
        (type SYMBOL)
        (create-accessor read-write))
    (slot modas
        (type SYMBOL)
        (create-accessor read-write))
    (slot edad
        (type INTEGER)                          
        (create-accessor read-write))
    (slot tiempo_lectura
        (type INTEGER)
        (create-accessor read-write))
    (multislot decada
        (type INTEGER)                          
